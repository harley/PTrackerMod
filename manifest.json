{
  "name": "Pivotal Tracker Mod",
  "version": "1.0.1",
  "description": "Tweaks to PivotalTracker UI.",
  "browser_action": {
    "default_icon": "images/icon.png",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs", "https://*.pivotaltracker.com/*", "http://*.pivotaltracker.com/*"
  ],
  "icons": { "16": "images/icon16.png",
             "48": "images/icon48.png",
            "128": "images/icon128.png" }
  
}

